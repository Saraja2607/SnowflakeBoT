pipeline {
    agent any
    // Stages begins here //
    stages {
        
        // Stage for Git Branches Checkout //
        stage('Checkout Git Branch') {
          echo "This is Checkout Git Branch"
        }
      
      stage('Deploy'){
          echo "This is Deployment Environment"
      }
    }
  posts {
    script {
      echo "This is Post script"
    }
  }
